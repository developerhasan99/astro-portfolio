---
import type { GetStaticPaths } from "astro";
import { getCollection } from "astro:content";
import MainLayout from "../../layouts/MainLayout.astro";

export const getStaticPaths = (async () => {
	const posts = await getCollection("project");

	return posts.map((post) => ({
		params: {
			slug: post.slug,
		},
		props: post,
	}));
}) satisfies GetStaticPaths;

const post = Astro.props;
const { Content } = await post.render();
---

<MainLayout>
	<div class="section-project">
		<div class="padding-global">
			<div class="container-large">
				<div class="padding-section-medium">
					<div class="project-component">
						<div class="project-content">
							<div class="caption text-color-gradient">Case study</div><div
								class="space-small"
							>
							</div><h1 class="heading-style-h3">{post.data.title}</h1><div
								class="space-medium"
							>
							</div><div class="text-color-grey-400">
								{post.data.description}
							</div><div class="space-medium"></div><a
								href="/projects"
								class="button white-icon w-inline-block"
								><img
									src="https://assets.website-files.com/634d6064d1c6401d462f6757/63713c580d7d56d05f9d3400_chevron-white-left.svg"
									loading="lazy"
									alt=""
									class="icon-small"
								/><div>Back to Projects</div></a
							>
						</div><div class="project-image-wrapper">
							<img
								src={post.data.heroImage}
								loading="lazy"
								alt=""
								sizes="(max-width: 479px) 93vw, (max-width: 767px) 44vw, (max-width: 991px) 43vw, (max-width: 1919px) 45vw, 832px"
								srcset="https://assets.website-files.com/6364e2710bb372fbb357f912/6364e2ded3daac2e14977780_joystick-blue-p-500.jpg 500w, https://assets.website-files.com/6364e2710bb372fbb357f912/6364e2ded3daac2e14977780_joystick-blue-p-800.jpg 800w, https://assets.website-files.com/6364e2710bb372fbb357f912/6364e2ded3daac2e14977780_joystick-blue-p-1080.jpg 1080w, https://assets.website-files.com/6364e2710bb372fbb357f912/6364e2ded3daac2e14977780_joystick-blue-p-1600.jpg 1600w, https://assets.website-files.com/6364e2710bb372fbb357f912/6364e2ded3daac2e14977780_joystick-blue.jpg 1920w"
								class="image"
							/>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
	<section class="section-project-body">
		<div class="padding-global">
			<div class="container-large">
				<div class="padding-section-medium">
					<div class="project-body-component">
						<div class="project-sidebar">
							<a href="#Overview" class="project-sidebar-link w-inline-block"
								>Overview</a
							>
							<a href="#Problem" class="project-sidebar-link w-inline-block"
								>Problem</a
							>
							<a href="#Solution" class="project-sidebar-link w-inline-block"
								>Solution</a
							>
							<a href="#UXResearch" class="project-sidebar-link w-inline-block"
								>UX Research</a
							>
							<a href="#Design" class="project-sidebar-link w-inline-block"
								>Design</a
							>
							<a href="#Development" class="project-sidebar-link w-inline-block"
